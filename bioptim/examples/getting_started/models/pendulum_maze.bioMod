segment NoGoCircle11
    mesh 0 $yeleven+$releven $zeleven
    mesh 0 $yeleven $zeleven+$releven
    mesh 0 $yeleven+(-$releven) $zeleven
    mesh 0 $yeleven $zeleven+(-$releven)
    mesh 0 $yeleven+$releven $zeleven
endsegment
