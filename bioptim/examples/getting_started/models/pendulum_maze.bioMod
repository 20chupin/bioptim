segment NoGoCircle0
    mesh 0 $y0+$r0 $z0  // $y10 or $z10 or $r10 or ${text}{any two numbers} in operations causes segfault
    mesh 0 $y0 $z0+$r0
    mesh 0 $y0+(-$r0) $z0
    mesh 0 $y0 $z0+(-$r0)
    mesh 0 $y0+$r0 $z0
endsegment
